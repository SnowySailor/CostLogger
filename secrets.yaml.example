# databaseconfig
  # Required: user
  #           host
  #           port
  #           database
  # Optional: password (default "")

# redisconfig
  # Required: host
  # Optional: port     (default 6379)
  #           password (default "")
  #           database (default 0)

# sessionconfig
  # Optional: secretkey (random default)
  #           name      (default "sessionid")
  #           maxage    (default 3600 seconds)
  #           httponly  (default "on")

#webconfig
  # Optional: maxuploadsize (default 32MB)
  # Optional: passwordstrength (default 10)


databaseconfig:
  username: "user"
  password: "password"
  database: "database"
  host:     "localhost"
  port:     5432
redisconfig:
  host:     "localhost"
  port:     6379
  password: ""
  database: 0
sessionconfig: 
  secretkey: "something very secret"
  name:      "session-name"
  maxage:    86400
  httponly:  "on"
webconfig:
  maxuploadsize: 100000000
  passwordstrength: 10 # Can be any integer between 4 and 31 (inclusive)
